/* Bouton pour expander/rÃ©duire une ligne - Style WordPress admin */
.me5rine-lab-table-toggle-btn {
    position: absolute;
    right: 12px;
    top: 50%;
    transform: translateY(-50%);
    width: 28px;
    height: 28px;
    border: 1px solid var(--me5rine-lab-border, #DEE5EC);
